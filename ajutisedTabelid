USE AdventureWorksDW2019;

select * from dimcustomer

-- teeme protseduuri, mis loob ajutise tabeli ja näitab seda
create procedure spcreatelocaltemptable
as 
begin
    -- loome kohaliku ajutise tabeli (# ainult selles sessioonis nähtav)
    create table #dimemployee (
        employeekey int,
        firstname nvarchar(20)
    )

    -- lisame andmed
    insert into #dimemployee values (1, 'mike')
    insert into #dimemployee values (2, 'john')
    insert into #dimemployee values (3, 'todd')


    select * from #dimemployee
end


exec spcreatelocaltemptable
